<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" name="ehcache">

	<diskStore path="${java.io.tmpdir}/${system.project_name}/cache" />

	<!-- DefaultCache setting. -->
	<defaultCache maxEntriesLocalHeap="10000" eternal="false" timeToIdleSeconds="300" timeToLiveSeconds="600"
		overflowToDisk="true" maxEntriesLocalDisk="100000" />

	<cache name="sysCache" maxElementsInMemory="10000" eternal="true" overflowToDisk="true" />

	<cache name="setting" maxElementsInMemory="100" eternal="true" overflowToDisk="true" />

</ehcache>
